<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutDirection="locale"
    android:background="@color/colorPrimary"
    tools:context=".practitioner.activities.PractitionerHomeActivity"
    tools:ignore="Overdraw">


    <FrameLayout
        android:id="@+id/practitionerHomeContainer"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/practitioner_bottom_nav"/>



    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/practitioner_bottom_nav"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemBackground="@color/colorPrimary"
        app:itemTextColor="@color/nav_items_color"
        app:itemIconSize="24dp"
        app:itemIconTint="@color/nav_items_color"
        app:menu="@menu/practitioner_nav_menu"
        android:clipChildren="false"
        app:labelVisibilityMode="labeled"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>